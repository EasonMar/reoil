{
  "name": "reoil",
  "description": "ðŸ§¡Reoil is a low-level react original component library which can be used as wapper to replace '<div>' '<img>' etc, also can be wrapper for any other components.",
  "keywords": [
    "react",
    "components",
    "ui",
    "library",
    "reoil",
    "styled"
  ],
  "version": "0.0.15",
  "main": "dist/index.js",
  "author": "Yokiijay <yokiijay@gmail.com> (https://yokiijay.github.io)",
  "license": "MIT",
  "files": [
    "dist"
  ],
  "scripts": {
    "build:ts": "yarn run tsc -p .",
    "build:types": "yarn run build:ts --emitDeclarationOnly",
    "build:babel": "yarn run babel src -d dist --extensions \".js,.jsx,.ts,.tsx\"",
    "build": "rm -rf dist && yarn run build:types && yarn run build:babel",
    "start": "yarn run build:types --watch & yarn run build:babel --source-maps inline --watch"
  },
  "peerDependencies": {
    "react": "^16.13.1"
  },
  "devDependencies": {
    "@babel/cli": "^7.10.4",
    "@babel/core": "^7.10.4",
    "@babel/preset-env": "^7.10.4",
    "@babel/preset-typescript": "^7.10.4",
    "@emotion/babel-preset-css-prop": "^10.0.27",
    "@types/react": "^16.9.41",
    "@types/resize-observer-browser": "^0.1.3",
    "@types/styled-system": "^5.1.9",
    "babel-preset-minify": "^0.5.1",
    "csstype": "^2.6.11",
    "react": "^16.13.1",
    "typescript": "^3.9.6"
  },
  "dependencies": {
    "@emotion/core": "^10.0.28",
    "@emotion/styled": "^10.0.27",
    "styled-system": "^5.1.5"
  }
}
